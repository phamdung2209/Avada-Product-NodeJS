{
  "presets": ["@babel/preset-react"],
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@assets": "./src",
          "@functions": "../functions/src"
        }
      }
    ]
  ]
}
