    "@shopify/polaris-icons": "^8.0.0",
    "axios": "^0.19.0",
    "firebase": "9.10.0",
    "history": "^4.10.1",
    "pegasus-loader": "^1.1.0",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-color": "2.19.3",
    "react-dom": "^18.2.0",
    "react-html-parser": "^2.0.2",
    "react-loadable": "^5.5.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^5.0.1",
    "react-router-routes-loader": "^0.2.1",
    "react-screentype-hook": "^1.0.18",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "react-photo-gallery": "8.0.0",
    "react-skela": "1.0.3",
    "@mageplaza/react-carousel": "1.1.0",
